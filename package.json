{
  "name": "react-zero-downtime-build",
  "version": "0.1.0",
  "description": "Eliminate chunk load errors and 404s during deployments with automatic version detection and user prompts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "rzd-build": "./bin/rzd-build.js",
    "rzd-init": "./bin/rzd-init.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vksco/react-zero-downtime-build.git"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "deployment",
    "zero-downtime",
    "build",
    "chunk-error",
    "404-error",
    "version-check",
    "hot-reload",
    "react-scripts",
    "create-react-app",
    "cache-busting",
    "auto-update"
  ],
  "homepage": "https://github.com/vksco/react-zero-downtime-build",
  "bugs": {
    "url": "https://github.com/vksco/react-zero-downtime-build/issues"
  },
  "files": [
    "dist",
    "bin",
    "templates",
    "README.md",
    "CHANGELOG.md"
  ],
  "author": "vksco",
  "engines": {
    "node": ">=14.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.0"
  }
}
